{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "format": "bunx prettier --write 'resources/**/*.{js,css,vue}' && php vendor/bin/pint",
        "format:check": "bunx prettier --check 'resources/**/*.{js,css,vue}' && php vendor/bin/pint --test",
        "build": "vite build",
        "dev": "vite"
    },
    "devDependencies": {
        "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
        "@prettier/plugin-oxc": "^0.0.4",
        "@tailwindcss/vite": "^4.0.0",
        "axios": "^1.11.0",
        "concurrently": "^9.0.1",
        "laravel-vite-plugin": "^2.0.0",
        "prettier": "^3.6.2",
        "prettier-plugin-blade": "^2.1.21",
        "prettier-plugin-ember-template-tag": "^2.1.0",
        "tailwindcss": "^4.0.0",
        "vite": "^7.0.4"
    },
    "dependencies": {
        "@ryangjchandler/alpine-clipboard": "^2.3.0"
    }
}
